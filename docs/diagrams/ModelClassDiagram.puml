@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Interface ReadOnlyTutorAid <<Interface>>
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class TutorAid
Class ReadOnlyTutorAid
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package Account {
Class Account
Class Username
}

Package Person {
Class Person
Class Attendance
Class Result
Class Participation
Class Name
Class Picture
Class UniquePersonList
}

Package ClassId {
Class ClassId
}

Package Task {
Class Task
Class Marking
Class TaskTime
Class TaskList
}

Package Commands {
Class CommandObject
Class CommandWord
Class CommandAction
Class UniqueCommandsList
}

Package Earnings {
Class Amount
Class Date
Class Earnings
Class Module
Class Type
Class UniqueEarningsList
}

Package Note {
Class Notes
Class Content
Class ModuleCode
Class UniqueNotesList
}

Package Reminder {
Class Reminder
Class ReminderDescription
Class ReminderTime
Class UniqueReminderList
}
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

TutorAid .up.|> ReadOnlyTutorAid

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" TutorAid
ModelManager o-left-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

TutorAid *--> Account
Account *--> Username

TutorAid *--> "1" UniquePersonList
UniquePersonList o--> "*" Person
Person *--> Attendance
Person *--> ClassId
Person *--> Result
Person *--> Participation
Person *--> Name
Person *--> Picture

TutorAid *--> "1" TaskList
TaskList o--> "*" Task
Task *--> ClassId
Task *--> Marking
Task *--> "*" TaskTime

TutorAid *--> "1" UniqueEarningsList
UniqueEarningsList o--> "*" Earnings
Earnings *--> Amount
Earnings *--> Module
Earnings *--> Date
Earnings *--> Type

TutorAid *--> "1" UniqueNotesList
UniqueNotesList o--> "*" Notes
Notes *--> ModuleCode
Notes *--> Content

TutorAid *--> "1" UniqueReminderList
UniqueReminderList o--> "*" Reminder
Reminder *--> ReminderDescription
Reminder *--> ReminderTime

TutorAid *--> "1" UniqueCommandsList
UniqueCommandsList o--> "*" CommandObject
CommandObject *--> CommandWord
CommandObject *--> CommandAction


ModelManager -->"1" Person : filtered list
ModelManager -->"1" CommandObject : filtered list
ModelManager -->"1" Task : filtered list
ModelManager -->"1" Earnings : filtered list
ModelManager -->"1" Reminder : filtered list
ModelManager -->"1" Notes : filtered list
@enduml
